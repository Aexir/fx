<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="anchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.dabkowski.edp.controllers.StreetListController">
    <VBox prefHeight="400.0" prefWidth="600.0">
        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="600.0">
            <Button fx:id="backButton" mnemonicParsing="false" onMouseClicked="#onBackButtonClick" prefHeight="31.0" prefWidth="60.0">
                <graphic>
                    <ImageView fitHeight="30.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@images/back.png" />
                    </ImageView>
                </graphic>
                <HBox.margin>
                    <Insets right="40.0" />
                </HBox.margin>
            </Button>
            <TextField fx:id="searchInput" prefHeight="25.0" prefWidth="317.0" promptText="Wyszukaj ulicÄ™">
                <HBox.margin>
                    <Insets left="10.0" right="10.0" />
                </HBox.margin>
            </TextField>
            <Button fx:id="searchButton" mnemonicParsing="false" onMouseClicked="#onSearchButtonClick" prefHeight="25.0" prefWidth="60.0">
                <graphic>
                    <ImageView fitHeight="30.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@images/lookup.png" />
                    </ImageView>
                </graphic>
            </Button>
        </HBox>
        <ListView fx:id="listView" onMouseClicked="#listViewOnMouseClickedEvent" prefHeight="370.0" prefWidth="600.0" />
    </VBox>
</AnchorPane>
